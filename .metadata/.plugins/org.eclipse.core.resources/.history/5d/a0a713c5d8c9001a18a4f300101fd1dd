package com.libreria.practicaClase6;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args ) {
    	
    }
    int fibo( int n ) {
    	

    	 int r;
    	 if ( n == 0 ) r = 0;
    	 else if ( n == 1 ) r = 1; else if ( n > 1 ) r = fibo(n-1) + fibo(n-2);
    	 return r;
    
    	} 
    {
     
    }
}
